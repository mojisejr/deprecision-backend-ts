extends base

block content 
  .list-container
    .list-header
      h1 products
    if products
      each product in products
        .product-card
          h1= product.model_no
          p= "type: " + product.type
          p= "id[ " + product._id + " ]"
          p= "createdAt: " + product.createdAt
          .button-box
            a.btn-detail(href="#") detail